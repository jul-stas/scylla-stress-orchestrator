#!/bin/python3

import sys
import os
sys.path.insert(1, f"{os.environ['SSO']}/src/")

import yaml
import argparse
from sso import terraform

if len(sys.argv) < 3:
	raise Exception("provision-terraform TERRAFORM_PLAN PROFILE_NAME")
else:
    terraform_plan = sys.argv[1]
    profile_name = sys.argv[2]

terraform.apply(terraform_plan, profile_name)
